cha';\n  letter-spacing: 4px;\n  font-weight: bold;\n  font-size: ${(props) => props.theme.fonts.MD};\n  color: red;\n`\n\nexport default function RegisterPage(){\n  const dispatch = useDispatch();\n  const [nickname, setNickname] = useState('');\n  const [username, setUsername] = useState('')\n  const [password, setPassword] = useState('')\n  const errorMessage = useSelector(store => store.user.errorMessage)\n  const setError = () => dispatch(setErrorMessage(null))\n  const history = useHistory();\n  useEffect(() => () => dispatch(setErrorMessage(null)), [dispatch]);\n  const handleSubmit = e => {\n    e.preventDefault();\n    dispatch(register(username, nickname, password)).then((res) => {\n      if (res) history.push(\"/\");\n    });\n  }\n    return (\n    <Form onSubmit={handleSubmit}>\n      {errorMessage && <ErrorMessage>{errorMessage}</ErrorMessage>}\n      <Wrapper>\n        nickname: <Input value={nickname} onFocus={setError} onChange={e => setNickname(e.target.value)}/>\n      </Wrapper>\n      <Wrapper>\n        username: <Input value={username} onFocus={setError} onChange={e => setUsername(e.target.value)}/>\n      </Wrapper>\n      <Wrapper>\n        password: <Input type=\"password\" value={password} onFocus={setError} onChange={e => setPassword(e.target.value)}/>\n      </Wrapper>\n      <Button type=\"submit\">註冊</Button>\n    </Form>\n  )\n}",["122","123"],"/Users/px732am/redux-blog/my-app/src/reportWebVitals.js",[],"/Users/px732am/redux-blog/my-app/src/pages/LoginPage/index.js",[],["124","125"],"/Users/px732am/redux-blog/my-app/src/pages/AboutPage/AboutPage.js",[],"/Users/px732am/redux-blog/my-app/src/utils.js",[],"/Users/px732am/redux-blog/my-app/src/pages/HomePage/HomePage.js",[],"/Users/px732am/redux-blog/my-app/src/redux/reducers/userReducer.js",[],"/Users/px732am/redux-blog/my-app/src/pages/HomePage/index.js",[],"/Users/px732am/redux-blog/my-app/src/pages/EditPostPage/index.js",[],"/Users/px732am/redux-blog/my-app/src/components/Header/index.